<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.kardihaekal.myfriends">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/friend"
        android:label="@string/app_name"
        android:roundIcon="@drawable/friend"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="id.kardihaekal.myfriends.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="id.kardihaekal.myfriends.EditorActivity"
          android:label="@string/add"
            android:theme="@style/EditorTheme">
        </activity>
    </application>

</manifest>